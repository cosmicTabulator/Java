package main;

public class Point3 {

	double x;
	double y;
	double z;
	
	public Point3(double x, double y, double z){
		
		this.x = x;
		this.y = y;
		this.z = z;
		
	}
	
	public static Point midpoint(Point a, Point b, Point c, Point d){
		
		Point out = new Point(0,0);
		
		out.x = ( a.x + b.x )/2;
		out.y = ( a.y + b.y )/2;
		
		return out;
	}
	
}
