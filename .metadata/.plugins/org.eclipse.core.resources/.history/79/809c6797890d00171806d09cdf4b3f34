package main;

public class Point3 {

	double x;
	double y;
	double z;
	
	public Point3(double x, double y, double z){
		
		this.x = x;
		this.y = y;
		this.z = z;
		
	}
	
	public static Point3 midpoint(Point3 a, Point3 b, Point3 c, Point3 d){
		
		Point3 out = new Point(0,0,0);
		
		out.x = ( a.x + b.x )/2;
		out.y = ( a.y + b.y )/2;
		
		return out;
	}
	
}
