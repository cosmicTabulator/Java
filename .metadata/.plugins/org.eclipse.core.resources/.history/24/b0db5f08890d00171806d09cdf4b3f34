package main;

import java.util.ArrayList;

public class List2D<T> {

	ArrayList<ArrayList<T>> superList = new ArrayList<ArrayList<T>>();
	
	public T get(int x, int y){
		
		T out = superList.get(x).get(y);
		
		return out;
		
	}
	
	public void put(int x, int y, T val){
		
		if(superList.get(x) == null){
			superList.add(x, new ArrayList<T>());
		}
		
		superList.get(x).add(y, val);
		
	}
	
	public String toString(){
		
		String out = new String();
		
		for(int i = 0; i < superList.size(); i++){
			for(int k = 0; k < superList.get(i).size(); k++){
				out += "(" + i + "," + k + "," + superList.get(i).get(k) + "), ";
			}
		}
		
		return out;
		
	}
}
