
public class Object {

	int x;
	int y;
	float xV = 10;
	float yV;
	int width = 10;
	int height = 10;
	
	public Object(int startX, int startY){
		
		x = startX;
		y = startY;
		
	}
	
	public void getPos(){
		x = (int) (x + xV);
		y = (int) (y + yV);
	}
	
	public void draw(){
		Draw d = new Draw();
		d.g.fillRect(x, y, width, height);
		d.refresh();
	}
	
}
