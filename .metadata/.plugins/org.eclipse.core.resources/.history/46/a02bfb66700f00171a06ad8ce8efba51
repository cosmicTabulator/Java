package graphics;

import java.awt.Color;

public class Cell {

	Color c;
	int x;
	int y;
	String tag;
	
	public Cell(){
		
	}
	
	public void setColor(Color color){
		
		this.c = color;
		
	}
	
	public void setTag(String s){
		
		this.tag = s;
		
	}
	
	public String getTag(){
		
		return tag;
		
	}
	
}
