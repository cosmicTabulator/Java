import java.util.ArrayList;
import java.util.List;

public class Cell {

	boolean status;
	
	Vector pos;
	
	public Cell(int x, int y, boolean state){
		pos.x = x;
		pos.y = y;
		this.status = state;
	}
	
	public List<Cell> getNeighbors(){
		
		List<Cell> neighbors = new ArrayList<Cell>();
		
		Vector n = new Vector(this.x, this.y -1);
		
		return neighbors;
		
	}
	
}
